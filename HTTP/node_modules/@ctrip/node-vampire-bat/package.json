{
  "name": "@ctrip/node-vampire-bat",
  "version": "1.1.10",
  "description": "node-vampire-bat",
  "main": "./libs/index.js",
  "types": "./types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test:0": "cross-env CONSOLE_DEBUG=* mocha -r ts-node/register 'test/**/*.spec.ts' --timeout=30000",
    "test:1": "nyc --cache --include 'src/utils/catThreadHelper/' mocha -r ts-node/register 'test/src/utils/catThreadHelper/*.spec.ts' --timeout=30000",
    "test": "npm run test:0 && npm run test:1",
    "test:each": "nyc mocha -r ts-node/register 'test/src/**/context.spec.ts' --timeout=30000",
    "coverage": "nyc --config .nycrc.json --cache --exclude 'src/utils/interface.ts'  --exclude 'src/utils/console.ts'  --exclude 'src/servlet/subProcess/subProcSender.ts' npm run test",
    "prepublishOnly": "rm -rf libs/ && rm -rf types/ && npm cache clean --force && npm install && npm run build",
    "build": "tsc"
  },
  "repository": "git@git.dev.sh.ctripcorp.com:fx-front-end/node-vampire.git",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "xh.jia@trip.com",
  "license": "ISC",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.16.2",
    "@types/proxyquire": "^1.3.31",
    "chai": "^4.2.0",
    "mocha": "^3.5.0",
    "nock": "^13.4.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.9.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2"
  },
  "dependencies": {
    "@ctrip/node-vampire-appconfig": "^1.0.0",
    "@ctrip/node-vampire-cache": "^1.0.0",
    "@ctrip/node-vampire-console": "^1.0.0",
    "@ctrip/node-vampire-foundation-framework": "^1.0.0",
    "@ctrip/node-vampire-hook": "^1.0.0",
    "@ctrip/node-vampire-hostinfo": "^1.0.0",
    "@ctrip/node-vampire-locker": "^1.0.0",
    "@ctrip/node-vampire-pkginfo": "^1.0.0",
    "@ctrip/node-vampire-util": "^1.0.0",
    "cross-env": "^7.0.3"
  },
  "tripPublisher": "xh.jia@trip.com"
}