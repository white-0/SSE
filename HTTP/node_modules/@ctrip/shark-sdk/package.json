{
  "name": "@ctrip/shark-sdk",
  "version": "5.1.15",
  "description": "Shark NodeJS SDK",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "eslint \"./lib/**/*.js\" --cache --cache-location=.cache/eslint",
    "fix": "eslint \"./lib/**/*.js\" --cache --cache-location=.cache/eslint --fix",
    "test": "mocha --timeout 30000 --reporter mochawesome --reporter-options reportDir=./coverage,reportFilename=test-result,html=false test/index.js --verbose --exit",
    "coverage": "nyc --reporter=text --reporter=lcov --reporter=json-summary npm test",
    "postinstall": "node lib/postinstall/install.js"
  },
  "repository": "git@git.dev.sh.ctripcorp.com:cloud-fe/cloud-shark.git",
  "keywords": [
    "shark",
    "node",
    "sdk"
  ],
  "nyc": {
    "all": true,
    "check-coverage": false,
    "exclude": [
      "src/**",
      "coverage/**",
      "scripts/**",
      "lib/log/**",
      "lib/adapter/**",
      "lib/provider/**",
      "lib/metric/**",
      "lib/timer/**",
      "lib/soa/**",
      "postinstall.js",
      "lib/ignition/slave.js"
    ]
  },
  "author": "jun.zhua",
  "license": "ISC",
  "pre-commit": [
    "lint",
    "test"
  ],
  "pre-push": [
    "lint",
    "coverage"
  ],
  "dependencies": {
    "@ctrip/node-vampire-cat": "^1.1.9",
    "@ctrip/node-vampire-foundation-framework": "^1.0.23",
    "@ctrip/node-vampire-locker": "^1.0.20",
    "@ctrip/node-vampire-pm2": "^1.0.2",
    "@ctrip/node-vampire-qconfig": "^1.0.48",
    "@ctrip/node-vampire-singleton": "^1.0.4",
    "@ctrip/node-vampire-standard-soa-client": "^1.0.9",
    "@ctrip/node-vampire-triplog": "^1.0.21",
    "@ctrip/shark-l10n": "^4.2.2",
    "bfj": "^7.1.0",
    "colors": "1.4.0",
    "cookie": "^0.6.0",
    "is-type-of": "^2.1.0",
    "object-sizeof": "^2.6.4",
    "piscina": "^4.3.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@ctrip/cargo-nodejs": "^3.6.1",
    "@types/koa": "^2.14.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "supertest": "^6.3.4"
  },
  "tripPublisher": "jun.zhua@trip.com"
}