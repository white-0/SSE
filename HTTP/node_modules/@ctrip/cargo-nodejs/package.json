{
  "name": "@ctrip/cargo-nodejs",
  "version": "3.6.1",
  "description": "Cargo NodeJS SDK",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "eslint \"./lib/**/*.js\" --cache --cache-location=.cache/eslint",
    "fix": "eslint \"./lib/**/*.js\" --cache --cache-location=.cache/eslint --fix",
    "test": "npm run build && __CTRIP_APP_CONFIG_FILE__=../../../test/app.config.js NODE_ENV=cargo_mock mocha --timeout 30000 -r jsdom-global/register --reporter mochawesome --reporter-options reportDir=./coverage,reportFilename=test-result,html=false --exit",
    "coverage": "nyc --reporter=text --reporter=lcov --reporter=json-summary npm test",
    "mock:update": "./test/generate-mock-data.sh ./test/mock",
    "debug": "NODE_ENV=cargo_mock mocha --inspect-brk ./test/index.js",
    "build": "rm -rf dist && webpack && rm -rf browser && babel lib/browser --extensions .js --out-dir browser",
    "prepublishOnly": "npm i && npm run build"
  },
  "author": "jun.zhua",
  "repository": "git@git.dev.sh.ctripcorp.com:cloud-fe/cloud-cargo.git",
  "nyc": {
    "all": true,
    "check-coverage": false,
    "exclude": [
      "app.config.js",
      "coverage/**",
      "demo/**",
      "lib/log/**",
      "lib/service/**",
      "lib/**/*abstract*.js",
      "lib/collector/heart-beat.js",
      "lib/**/*slave*.js",
      "lib/middlewares/*.js",
      "lib/cache/client/*.js"
    ]
  },
  "license": "ISC",
  "pre-commit": [
    "lint",
    "test"
  ],
  "pre-push": [
    "lint",
    "coverage"
  ],
  "dependencies": {
    "@ctrip/node-vampire-appconfig": "^1.0.12",
    "@ctrip/node-vampire-clogging": "^1.0.11",
    "@ctrip/node-vampire-pm2": "^1.0.2",
    "@ctrip/node-vampire-singleton": "^1.0.4",
    "@ctrip/node-vampire-standard-soa-client": "^1.0.9",
    "@ctrip/node-vampire-triplog": "^1.0.18",
    "colors": "1.4.0",
    "cookie": "^0.6.0",
    "is-type-of": "^2.1.0",
    "write-file-atomic": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.13.12",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "express": "^4.18.2",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "tripPublisher": "jun.zhua@trip.com"
}