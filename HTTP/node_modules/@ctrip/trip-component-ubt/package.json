{
  "name": "@ctrip/trip-component-ubt",
  "version": "4.1.4",
  "description": "trip component ubt 1.0 helper. Not support ubt2.0",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "keywords": [
    "trip",
    "component",
    "test"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:lib && npm run build:es",
    "clean": "rm -rf es lib",
    "build:lib": "tsc --project tsconfig.prod.json",
    "build:es": "npm run build:lib -- --module es6 --outDir es",
    "test": "echo \"Error: no test specified\"",
    "tripNpmPublish": "rm -rf ./node_modules ../../node_modules package-lock.json",
    "prepublishOnly": "npm run tripNpmPublish && npm i --legacy-peer-deps=false && npm run build"
  },
  "author": "http://git.dev.sh.ctripcorp.com/trip-component",
  "repository": "git@git.dev.sh.ctripcorp.com:trip-component/trip-component-framework.git",
  "dependencies": {
    "@ctrip/trip-component-utils": "^4.1.4"
  },
  "devDependencies": {
    "typescript": "^3.8.2"
  },
  "files": [
    "lib/**",
    "es/**"
  ],
  "tripPublisher": "j.yangf@trip.com"
}