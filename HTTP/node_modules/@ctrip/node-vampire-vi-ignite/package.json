    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@ctrip/node-vampire-cat": "^1.1.2",
    "@ctrip/node-vampire-foundation-framework": "^1.0.20",
    "@ctrip/node-vampire-hook": "^1.0.3",
    "@ctrip/node-vampire-hostinfo": "^1.0.1",
    "@ctrip/node-vampire-http": "^1.0.12",
    "@ctrip/node-vampire-metric": "^1.0.6",
    "@ctrip/node-vampire-triplog": "^1.0.16",
