{
  "name": "@ctrip/node-vampire-vi-ignite",
  "version": "1.0.45",
  "description": "vi ignite check",
  "main": "libs/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "npm run test1 && npm run test2 && mocha --exit -t 20000 -R spec test/igniteRuntime.spec.js",
    "test1": "mocha --exit -t 20000 -R spec test/igniteSuccess.spec.js --exit",
    "test2": "mocha --exit -t 20000 -R spec test/index.spec.js --exit",
    "build": "tsc -p src/tsconfig.json",
    "run:dev": "node test/test.js --ivDev",
    "prepublishOnly": "rm -rf libs/ && rm -rf types/ && npm cache clean --force && npm install && npm run build && rm -rf libs/tsconfig.tsbuildinfo"
  },
