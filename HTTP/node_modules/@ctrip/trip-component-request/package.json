{
  "name": "@ctrip/trip-component-request",
  "version": "4.2.1",
  "description": "trip component request",
  "main": "lib/node/index.js",
  "module": "es/node/index.js",
  "browser": {
    "./browser.js": "./es/browser/index.js",
    "./lib/node/index.js": "./lib/browser/index.js",
    "./es/node/index.js": "./es/browser/index.js"
  },
  "types": "index.d.ts",
  "sideEffects": false,
  "keywords": [
    "trip",
    "component",
    "request"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint:type && cp -R lib es && npm run build:lib && npm run build:es",
    "clean": "rm -rf es lib",
    "lint:type": "tsc --project tsconfig.prod.json",
    "build:lib": "babel src --extensions .ts,.tsx --out-dir lib --ignore src/node/**/*",
    "build:es": "ES=\"true\" babel src --extensions .ts,.tsx --out-dir es",
    "test": "echo \"Error: no test specified\"",
    "tripNpmPublish": "rm -rf ./node_modules ../../node_modules package-lock.json",
    "prepublishOnly": "npm run tripNpmPublish && npm i --legacy-peer-deps=false && npm run build"
  },
  "author": "http://git.dev.sh.ctripcorp.com/trip-component",
  "repository": "git@git.dev.sh.ctripcorp.com:trip-component/trip-component-framework.git",
  "peerDependencies": {
    "@ctrip/cargo-nodejs": ">=3",
    "@ctrip/node-vampire-soa": "^1",
    "@ctrip/node-vampire-soa-client": "^1"
  },
  "dependencies": {
    "@ctrip/trip-component-ubt": "^4.1.4",
    "@ctrip/trip-component-utils": "^4.1.6",
    "@fastly/performance-observer-polyfill": "^1.1.0",
    "@types/koa": "^2.11.3",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@ctrip/cargo-nodejs": "^3",
    "@ctrip/node-vampire-soa": "^1",
    "@ctrip/node-vampire-soa-client": "^1",
    "@types/node": "^12.0.5",
    "typescript": "^3.8.2"
  },
  "overrides": {
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.20"
  },
  "files": [
    "lib/**",
    "es/**",
    "dist/**",
    "doc/**",
    "examples/**",
    "browser.js",
    "browser.d.ts",
    "node/**",
    "index.d.ts"
  ],
  "tripPublisher": "j.yangf@trip.com"
}